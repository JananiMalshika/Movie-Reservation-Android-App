<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/background">

    <include
        android:id="@+id/include2"
        layout="@layout/toolbar" />

    <View
        android:id="@+id/acc_info"
        android:layout_width="357dp"
        android:layout_height="39dp"

        android:layout_marginStart="34dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="20dp"
        android:background="#2B3543"
        android:backgroundTintMode="add"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="34dp" />

    <View
        android:id="@+id/my_reserv"
        android:layout_width="357dp"
        android:layout_height="39dp"

        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="#2B3543"
        android:backgroundTintMode="add"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/acc_info"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="34dp" />

    <View
        android:id="@+id/theater_rating"
        android:layout_width="357dp"
        android:layout_height="39dp"

        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="#2B3543"
        android:backgroundTintMode="add"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/my_reserv"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="34dp" />

    <View
        android:id="@+id/privecy"
        android:layout_width="357dp"
        android:layout_height="39dp"

        android:layout_marginStart="34dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:background="#2B3543"
        android:backgroundTintMode="add"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/theater_rating"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="34dp" />

    <TextView
        android:id="@+id/changePhoto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/userName"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView2" />

    <View
        android:id="@+id/header_divider5"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginBottom="48dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="@color/white"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.cardview.widget.CardView

        android:id="@+id/cardView2"
        android:layout_width="81dp"
        android:layout_height="81dp"
        android:layout_margin="1dp"
        android:layout_marginTop="1dp"
        android:elevation="10dp"
        app:cardCornerRadius="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include2">

        <ImageView
            android:id="@+id/acc_userimg"
            android:layout_width="140dp"
            android:layout_height="92dp"
            android:layout_marginLeft="-30dp"
            android:layout_marginTop="-5dp"
            android:layout_marginBottom="-10dp"

            android:src="@drawable/userimg" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/userEmail"
        android:textColor="@color/light_blue"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changePhoto2" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="72dp"
        android:text="@string/totalpoints"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/totWatch"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginLeft="120dp" />

    <View
        android:id="@+id/divider2"
        android:layout_width="3dp"
        android:layout_height="70dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="20dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="@color/grey"
        app:layout_constraintEnd_toStartOf="@+id/totWatch"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Total Watched"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/totWatch" />

    <TextView
        android:id="@+id/totWatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="104dp"
        android:text="@string/totwatched"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginRight="104dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Total Points"
        android:textColor="@color/grey"
        app:layout_constraintEnd_toStartOf="@+id/totWatch"
        app:layout_constraintHorizontal_bias="0.645"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="84dp"
        android:text="@string/acc_info"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/acc_info"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/acc_info"
        app:layout_constraintVertical_bias="0.533"
        android:layout_marginRight="84dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginEnd="128dp"
        android:text="@string/my_reserv"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/my_reserv"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/my_reserv"
        android:layout_marginRight="128dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/theater_rating"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/theater_rating"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.238"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/theater_rating" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:text="@string/privacy"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/privecy"
        app:layout_constraintEnd_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/privecy"
        android:layout_marginRight="140dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/acc_info"
        app:layout_constraintEnd_toStartOf="@+id/textView19"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="@+id/acc_info"
        app:layout_constraintTop_toTopOf="@+id/acc_info"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_outline_account_circle_24"
        android:layout_marginLeft="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/acc_info"
        app:layout_constraintEnd_toEndOf="@+id/acc_info"
        app:layout_constraintTop_toTopOf="@+id/acc_info"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_baseline_navigate_next_24"
        android:layout_marginRight="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/my_reserv"
        app:layout_constraintEnd_toEndOf="@+id/my_reserv"
        app:layout_constraintTop_toTopOf="@+id/my_reserv"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_baseline_navigate_next_24"
        android:layout_marginRight="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/theater_rating"
        app:layout_constraintEnd_toEndOf="@+id/theater_rating"
        app:layout_constraintTop_toTopOf="@+id/theater_rating"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_baseline_navigate_next_24"
        android:layout_marginRight="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/privecy"
        app:layout_constraintEnd_toEndOf="@+id/privecy"
        app:layout_constraintTop_toTopOf="@+id/privecy"
        app:srcCompat="@drawable/ic_baseline_navigate_next_24"
        android:layout_marginRight="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/my_reserv"
        app:layout_constraintEnd_toStartOf="@+id/textView20"
        app:layout_constraintHorizontal_bias="0.019"
        app:layout_constraintStart_toStartOf="@+id/my_reserv"
        app:layout_constraintTop_toTopOf="@+id/my_reserv"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_outline_monetization_on_24"
        android:layout_marginLeft="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/theater_rating"
        app:layout_constraintStart_toStartOf="@+id/theater_rating"
        app:layout_constraintTop_toTopOf="@+id/theater_rating"
        app:layout_constraintVertical_bias="0.533"
        app:srcCompat="@drawable/ic_outline_feedback_24"
        android:layout_marginLeft="8dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/privecy"
        app:layout_constraintEnd_toStartOf="@+id/textView22"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="@+id/privecy"
        app:layout_constraintTop_toTopOf="@+id/privecy"
        app:srcCompat="@drawable/ic_outline_insert_drive_file_24"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_divider5"
        app:srcCompat="@drawable/ic_baseline_support_24"
        android:layout_marginLeft="36dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/header_divider5"
        app:srcCompat="@drawable/ic_baseline_local_movies_24"
        android:layout_marginLeft="68dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toBottomOf="@+id/header_divider5"
        app:srcCompat="@drawable/ic_baseline_fastfood_24"
        android:layout_marginLeft="80dp"
        tools:ignore="VectorDrawableCompat" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toEndOf="@+id/imageView15"
        app:layout_constraintTop_toBottomOf="@+id/header_divider5"
        app:srcCompat="@drawable/ic_baseline_account_circle_24"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/header_profile"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/include2"
        app:layout_constraintEnd_toEndOf="@+id/include2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>